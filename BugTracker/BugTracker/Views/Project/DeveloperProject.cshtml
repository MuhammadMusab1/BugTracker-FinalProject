    ViewData["Title"] = "DeveloperProject";
}

<h1>Developer Project</h1>

<div>
    <h4>Project</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
